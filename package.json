  "bugs": {
    "url": "https://github.com/mulesoft/ui-libs/issues"
  },
  "devDependencies": {
    "typescript": "1.8.7",
    "typings": "^0.5.1",
    "webpack": "^1.3.7",
    "json-loader":"^0.5.1"
  }
}
