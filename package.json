{
  "name": "atom-ui-lib",
  "version": "0.0.3",
  "private": false,
  "main": "dist/UI.js",
  "scripts": {
    "test-cov": " ./node_modules/.bin/istanbul cover _mocha tests/*Tests.js",
    "typings-for-idea": "node ./tools/createDeclarations",
